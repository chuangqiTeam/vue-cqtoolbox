{
  "presets": [
    "es2015",
    "es2017",
    "stage-0",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie <= 8"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "add-module-exports"
  ]
}
